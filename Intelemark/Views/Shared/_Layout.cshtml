    <script type="text/javascript">
        var sTime;
        $(function () {
            var signalrhub = $.connection.notificationHub;

            signalrhub.client.displayStatus = function (message, style, alignment, timeout) {
                console.log("alert")
                toastr.options = {
                    "closeButton": true,
                    "debug": false,
                    "newestOnTop": false,
                    "progressBar": false,
                    "positionClass": alignment,
                    "preventDuplicates": false,
                    "onclick": null,
                    "showDuration": "300",
                    "hideDuration": "1000",
                    "timeOut": timeout,
                    "extendedTimeOut": "1000",
                    "showEasing": "swing",
                    "hideEasing": "linear",
                    "showMethod": "fadeIn",
                    "hideMethod": "fadeOut"
                }

                toastr[style](message);
            }

            signalrhub.client.sweetAlert = function () {
                swal({
                    title: 'Warning',
                    text: "There are no more contacts left to call with your current settings.",
                    icon: "warning",
                    buttons: {
                        catch: {
                            text: "Ok",
                            value: true,
                            className: "btn-success"
                        }
                    }

                })
            }

            $.connection.hub.start().done(function () {
                @if((int?)Session["CampaignId"] != null)
                {
                    var x = ((int)Session["CampaignId"]);
                    <text>
                         signalrhub.server.sendCampaign(@x);
                    </text>
                }
            });
        });

        /** add active class and stay opened when selected */
        var url = window.location;

        // for sidebar menu entirely but not cover treeview
        $('li a.menu-item').filter(function () {
            return this.href == url;
        }).parent().addClass('active');

        // for treeview
        $('.menu-content > li').filter(function () {
            return this.href == url;
        }).parentsUntil("navigation-main > li.nav-item").addClass('has-sub open');

    </script>

    <!--END Signalr -->
